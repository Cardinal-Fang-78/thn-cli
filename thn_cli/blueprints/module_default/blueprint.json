{
    "name": "module_default",
    "version": 1,
    "description": "Default scaffold for creating a new THN Project Module.",
    "variables": {
        "module_name": "str",
        "project_name": "str",
        "created": "str"
    },
    "templates": [
        {
            "source": "templates/module_info.txt.j2",
            "destination": "{{ project_name }}/modules/{{ module_name }}/README.txt"
        },
        {
            "source": "templates/structure.json.j2",
            "destination": "{{ project_name }}/modules/{{ module_name }}/structure.json"
        }
    ]
}
